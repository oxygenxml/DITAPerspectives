// DITA Specialization glossgroup
strict digraph {
				
  // Root node
  
	"glossgroupModule" [label ="glossgroupModule" fontname="Arial" shape="Mrecord" style="filled"  fillcolor="#DDDDDD"]
			
  // Other nodes
      
	"concept" [label ="concept" fontname="Arial" shape="Mrecord" style="filled"  fillcolor="#AFA7F4"]
         
	"glossgroup" [label ="glossgroup" fontname="Arial" shape="Mrecord" style="filled"  fillcolor="#1AFABC"]
      
   
  // Edges
      
	"glossgroupModule" -> "concept" [fillcolor="#a6cee3" color="#1f78b4"]
         
	"concept" -> "glossgroup" [fillcolor="#a6cee3" color="#1f78b4"]
      
   }